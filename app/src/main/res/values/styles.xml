<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EstiloBotonNumero" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@android:color/background_dark</item>
    </style>
    <style name="EstiloTexto" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:gravity">center|right</item>
    </style>
    <style name="EstiloBotonClear" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">@android:color/holo_red_dark</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@android:color/background_dark</item>
    </style>
    <style name="EstiloBotonOperacion" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#009988</item>
    </style>
    <style name="EstiloBotonIgual" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@android:color/holo_orange_light</item>
    </style>
    <style name="EstiloFondoAPP" parent="@android:style/Theme.Dialog">
        <item name="android:background">@android:color/background_dark</item>
    </style>
